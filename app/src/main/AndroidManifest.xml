<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.kpetshop">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pet"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity
            android:name=".AdminUserOrderActivity"
            android:parentActivityName=".AdminCheckOrderActivity" />
        <activity android:name=".AdminCheckOrderActivity"
            android:parentActivityName=".AdminCategoryActivity" />
        <activity android:name=".AdminAddProductActivity" />
        <activity android:name=".ConfirmActivity"
            android:parentActivityName=".CartActivity" />
        <activity android:name=".DetailProductActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".SettingActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".SearchActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".CartActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".AdminCategoryActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SignupActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".LoginActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>